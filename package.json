{
  "name": "@runnable/cli",
  "version": "2.0.1",
  "description": "Runnable CLI",
  "homepage": "https://runnable.io",
  "repository": "https://github.com/Runnable/cli",
  "bin": {
    "runnable": "./bin/runnable.js"
  },
  "scripts": {
    "coverage": "istanbul cover node_modules/.bin/_mocha -- --recursive && npm run coverage-check",
    "coverage-check": "istanbul check-coverage && echo 'Coverage check successful!'",
    "feature": "cucumber-js",
    "format": "standard-format --write --verbose",
    "test": "npm run lint && npm run unit && npm run feature",
    "lint": "standard --verbose",
    "unit": "mocha --recursive"
  },
  "author": "Bryan Kendall <bryan@runnable.com>",
  "license": "MIT",
  "standard": {
    "globals": [
      "describe",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it"
    ]
  },
  "dependencies": {
    "101": "^1.3.1",
    "@runnable/api-client": "^5.0.0",
    "bluebird": "^3.3.1",
    "colors": "^1.0.3",
    "commander": "^2.7.1",
    "console.table": "^0.4.0",
    "debug": "^2.1.1",
    "docker-stream-cleanser": "^0.3.0",
    "easy-table": "^1.0.0",
    "inflect": "^0.3.0",
    "keypather": "^1.8.1",
    "parse-github-url": "^0.3.0",
    "read": "^1.0.7",
    "request": "^2.69.0",
    "simple-git": "^1.24.0",
    "substream": "^0.2.0",
    "tough-cookie-filestore": "0.0.1",
    "uuid": "^2.0.1",
    "ws": "^1.0.1"
  },
  "devDependencies": {
    "basic-auth": "^1.0.3",
    "body-parser": "^1.15.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "cucumber": "^0.9.4",
    "docker-frame": "^1.0.0",
    "express": "^4.13.4",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "primus": "^4.0.5",
    "sinon": "^1.17.3",
    "sinon-as-promised": "^4.0.0",
    "standard": "^6.0.5",
    "standard-format": "^2.1.0"
  }
}
